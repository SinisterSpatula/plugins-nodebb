{
  "name": "nodebb-plugin-avatargallery",
  "version": "1.0.0",
  "description": "This plugin allows admins to upload a gallery of images and then users can select one of them as their avatar.",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SinisterSpatula/nodebb-plugin-avatargallery"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "avatar",
    "gallery",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SinisterSpatula/nodebb-plugin-avatargallery/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-angular": "19.3.0",
    "eslint": "9.x",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.x",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
}
