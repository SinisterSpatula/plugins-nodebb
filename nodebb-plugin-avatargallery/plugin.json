{
  "id": "nodebb-plugin-avatargallery",
  "url": "https://github.com/SinisterSpatula/nodebb-plugin-avatargallery",
  "library": "./library.js",
  "hooks": [
    { "hook": "static:app.load", "method": "init" },
    { "hook": "filter:uploadImage", "method": "uploadImage" },
    { "hook": "action:plugin.activate", "method": "activate" },
    { "hook": "action:plugin.deactivate", "method": "deactivate" }
  ],
  "staticDirs": {
    "static": "./static"
  },
  "scss": ["scss/quickstart.scss"],
  "scripts": ["public/lib/main.js"],
  "acpScripts": ["public/lib/acp-main.js"],
  "modules": {
    "../client/quickstart.js": "./public/lib/quickstart.js",
    "../admin/plugins/quickstart.js": "./public/lib/admin.js"
  },
  "templates": "templates"
}
