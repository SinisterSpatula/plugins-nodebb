{
  "id": "nodebb-plugin-avatargallery",
  "url": "https://github.com/SinisterSpatula/nodebb-plugin-avatargallery",
  "library": "./library.js",
  "hooks": [
    { "hook": "static:app.load", "method": "init" },
    { "hook": "filter:admin.header.build", "method": "addAdminNavigation" }
  ],
  "staticDirs": {
    "static": "./static"
  },
  "scss": ["static/scss/quickstart.scss"],
  "scripts": ["static/lib/main.js"],
  "acpScripts": ["static/lib/acp-main.js"],
  "modules": {
    "../client/quickstart.js": "./static/lib/quickstart.js",
    "../admin/plugins/quickstart.js": "./static/lib/admin.js"
  },
  "templates": "./templates"
}
